<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 "><blockquote>
        <%= profile_img(user) %>
        <%= link_to user.name, user_path(user.id) %>
        <%= render 'follow_form', user: user %></blockquote>
      </div>
  <% end %>
</div>